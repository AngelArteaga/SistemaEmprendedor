@using Sistemaemprendedor.Models
@model EcosistemaModelo
@{
    ViewBag.Title = "Infografia";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <ol class="tg-breadcrumb">
                <li><a href="@Url.Action("Index", "Home")">Inicio</a></li>
                <li><a href="@Url.Action("Organizaciones", "Catalogo")">Organizaciones</a></li>
                <li class="tg-active">@Model.Region.Nombre</li>
            </ol>
        </div>
    </div>
</div>

<!--************************************
                    Grid Start
            *************************************-->
<section class="tg-haslayout">
    <div class="container">
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div class="mobileHide" style="width: 100%;">
                    <div style="width: 100%;">
                        <div style="position: relative; padding-bottom: 77.11%; padding-top: 0; height: 0;">
                            <iframe frameborder="0" width="1573" height="1213" style="position: absolute; top: 0; left: 0; width: 100%; height: 100%;" src="@Model.Region.Infografia" type="text/html" allowscriptaccess="always" allowfullscreen="true" scrolling="yes" allownetworking="all"></iframe>
                        </div>
                    </div>
                </div>                
            </div>
        </div>
    </div>

</section>
<!--************************************
        Grid End
*************************************-->

